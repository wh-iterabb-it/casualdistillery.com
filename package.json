{
  "name": "casual-site",
  "version": "0.0.1",
  "description": "A basic react site for reuse",
  "main": "./src",
  "scripts": {
    "start": "npm run build && export NODE_ENV=production node_modules/.bin/babel-node --presets react,es2015 src/server.js",
    "start:dev": "export NODE_ENV=development && npm run build:dev && nodemon --exec node_modules/.bin/babel-node -- src/server.js",
    "start:universal": "export UNIVERSAL=true && npm run start",
    "start:dev:universal": "export NODE_ENV=development && export UNIVERSAL=true && npm run start:dev",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p",
    "build:dev": "node_modules/.bin/webpack -d",
    "build:dev:watch": "node_modules/.bin/webpack -d --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wh-iterabb-it/Casual-Site.git"
  },
  "keywords": [
    "react",
    "node",
    "es6"
  ],
  "author": "Beau Bouchard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wh-iterabb-it/Casual-Site/issues"
  },
  "homepage": "https://github.com/wh-iterabb-it/Casual-Site#readme",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "ejs": "^2.5.6",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "3.0.1",
    "eslint-plugin-react": "5.2.2",
    "express": "^4.14.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.6.1"
  },
"devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "cli-color": "^1.1.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.14.0",
    "express-ws": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob-fs": "^0.1.6",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-cli": "^1.2.2",
    "gulp-eslint": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-webpack": "^1.5.0",
    "highlight.js": "^9.9.0",
    "http-server": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "meta-marked": "^0.4.2",
    "rupture": "^0.6.2",
    "sitemap-webpack-plugin": "^0.1.2",
    "slug": "^0.9.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "through": "^2.3.8",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.9.0",
